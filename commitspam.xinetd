service commitspam-stream
{
        disable     = no
        socket_type = stream
        protocol    = tcp
        port        = 8060
        type        = unlisted
        user        = www-data
        group       = www-data
        wait        = no
        server      = /opt/commitspam/spawn.rb
        only_from   = 192.30.25.0/22 204.232.175.64/27 88.217.184.122/32 127.0.0.1/32
        log_on_success = PID HOST DURATION USERID EXIT
        log_on_failure = HOST USERID ATTEMPT
}

